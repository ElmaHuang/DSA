public class Filter(){
  String input_line = null;
  public Filter(String input_fileName,String Output_fileName_1,String Output_fileName_2,String Output_fileName_3){
    try{
      //read file
      FileReader fileReader = new FileReader(input_fileName);
      BufferReader bufferReader = new BufferReader(input_fileName);
      
      //filer
      while((input_line = bufferReader.readLine()) != null){
        Output_Name = Filter(input_line);    
      }
      
      //write file
      FileOutputStream outputStream =new FileOutputStream(Output_Name);
      outputStream.write(input_line);
      
    }catch(FileNotFoundException e){
         System.out.println("Unable to open file fileName ");
         
    }catch(IOException e){
        System.out.println("Error read/writing file");      
      
    }
  }
